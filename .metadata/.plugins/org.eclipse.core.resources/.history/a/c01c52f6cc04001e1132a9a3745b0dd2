package test;

import java.time.LocalDate;
import java.util.*;
// model
public class QService {
	
	Map<String , Q> qList = new HashMap<> ();
	
	
	public QService () {
		Q q1 = new Q("1001","닭가슴살1", 1500 , LocalDate.of(2024, 1, 1) , 8 , 3 , 23);
		Q q2 = new Q("1002","닭가슴살2", 1700 , LocalDate.of(2024, 1, 2) , 7 , 4 , 20);
		Q q3 = new Q("1003","닭가슴살3", 2000 , LocalDate.of(2024, 1, 3) , 6 , 5 , 22);
		
		qList.put("1", q1);
		qList.put("2", q2);
		qList.put("3", q3);
	}
	
	public List<Q> All () {
		return new ArrayList<>(qList.values());
	}
	
	
	public Q find(String id) {
		return qList.get(id);
	}
	
	
	
	
 	
	
	
	
	
}
