package test;

import java.util.*;
// model
public class QService {
	
	Map<String , Q> qList = new HashMap<> ();
	
	
	public QService () {
		Q q1 = new Q("101","김지우","서울시","silver","010-1111-1111");
		Q q2 = new Q("102","홍길동","인천시","gold","010-2222-2222");
		Q q3 = new Q("103","율곡","김포시","vip","010-3333-3333");
		
		qList.put("1", q1);
		qList.put("2", q2);
		qList.put("3", q3);
	}
	
	public List<Q> All () {
		return new ArrayList<>(qList.values());
	}
	
	
	public Q find(String id) {
		return qList.get(id);
	}
	
	
	
	
 	
	
	
	
	
}
